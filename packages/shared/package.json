    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rimraf dist node_modules"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
